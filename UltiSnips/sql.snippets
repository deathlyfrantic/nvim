snippet proc "stored procedure for SQL Server" b
IF OBJECT_ID ('${2:<database>}.${1:<procedure>}', 'P') IS NOT NULL
	DROP PROCEDURE $2.$1
GO

CREATE PROCEDURE $2.$1
	${3:<parameters>}
AS
	$0
GO
endsnippet

snippet tf "table value function for SQL Server" b
IF OBJECT_ID ('${2:<database>}.${1:<function>}', 'IF') IS NOT NULL
	DROP FUNCTION $2.$1
GO

CREATE FUNCTION $2.$1(${3:<parameters>})
RETURNS TABLE
AS
	RETURN
		$0
GO
endsnippet
